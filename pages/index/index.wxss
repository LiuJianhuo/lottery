.mask {
  position: fixed;
  width: 100vw;
  height: 100vh;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
  background-color: #000;
  opacity: .6;
  z-index: 25000;
  display: none;
}

.mask.active {
  display: block;
}

.share {
  position: fixed;
  top: 0;
  left: 50%;
  width: 730rpx;
  height: 420rpx;
  z-index: 35000;
  transform: translateX(-50%);
  display: none;
}

.share.active {
  display: block;
}

.share-tabbar {
  position: fixed;
  width: 100%;
  bottom: -220rpx;
  height: 220rpx;
  background-color: #fff;
  display: flex;
  align-items: center;
  justify-content: space-betweens;
  font-size: 22rpx;
  border-top-right-radius: 8rpx;
  border-top-left-radius: 8rpx;
  z-index: 40000;
  transform: translateY(100%)
}

.share-tabbar .item {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  width: 50%;
  height: 100%;
  margin-top: -10rpx;
}

.share-tabbar .item .icon{
  width: 80rpx;
  height: 80rpx;
  border-radius: 50%;
}
.share-tabbar .item text {
  margin-top: 12rpx;
}
.share-tabbar .item .icon-wrap {
  box-sizing: border-box;
  width: 80rpx;
  height: 80rpx;
  background: linear-gradient(0deg,#df7339, #f18c56);
  border-radius: 50%;
  display: flex;
  align-items: center;
  justify-content: center;
}

.share-tabbar .item .icon.save-pic {
  width: 35rpx;
  height: 28rpx;
  border-radius: 0;
}

.share-tabbar .item button[plain] {
  border: none;
  padding: 0;
  background: linear-gradient(0deg,#fff, #fff);
  margin: 0;
} 

