/* pages/wx-official-acc/index.wxss */
page {
    background-color: #fff;
}

.title-wrap {
  margin-top: 40rpx;
  text-align: center;
  margin-bottom: 40rpx;
}
.title {
  display: block;
  line-height: 50rpx;
}
.banner-img-wrap {
  box-sizing: border-box;
  padding: 0px 80px;
}
.banner-img {
    width: 100%;
    min-height: 600rpx;
    display: block;
}

.btn-open {
    height: 100rpx;
    border-radius: 12rpx;
    margin-top: 30rpx;
    margin-bottom: 20rpx;
    color: rgb(192, 186, 186);
    background-color: #dedede;
}

button.btn-open {
  line-height: 100rpx;
  padding: 0;
}

.tip {
    font-size: 24rpx;
    color: #aaa;
    text-align: center;
    letter-spacing: 1px;
}